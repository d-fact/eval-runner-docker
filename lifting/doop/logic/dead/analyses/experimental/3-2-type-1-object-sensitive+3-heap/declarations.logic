MySystemThreadGroup(?type1, ?type2, ?heap, ?alloc) ->
  Type(?type1), Type(?type2), HeapAllocation(?heap),
  HeapAllocation(?alloc).

MyMainThreadGroup(?type1, ?type2, ?heap, ?alloc) ->
  Type(?type1), Type(?type2), HeapAllocation(?heap),
  HeapAllocation(?alloc).

MyMainThread(?type1, ?type2, ?heap, ?alloc) ->
  Type(?type1), Type(?type2), HeapAllocation(?heap),
  HeapAllocation(?alloc).
